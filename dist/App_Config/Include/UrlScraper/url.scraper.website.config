<?xml version="1.0" encoding="utf-8" ?>

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <sites>
      <site name="conecta" patch:before="site[@name='website']"
            hostName="url-scraper.dev.local"
            virtualFolder="/"
            physicalFolder="/"
            rootPath="/sitecore/content/RootSite"
            startItem="/ScraperPage"
            database="web"
            domain="extranet"
            allowDebug="true"
            cacheHtml="true"
            htmlCacheSize="50MB"
            enablePreview="true"
            enableWebEdit="true"
            enableDebugger="true"
            language="pt-BR"
            disableClientData="false"/>
    </sites>
  </sitecore>
</configuration>